{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      title: \"FastAPI And Vue Login\",\n      username: \"\",\n      password: \"\",\n      showForm: \"register\",\n      registrationResult: \"\",\n      loginResult: \"\"\n    };\n  },\n  methods: {\n    submitForm: function (formType) {\n      if (formType === \"register\") {\n        axios.post(\"/api/register\", {\n          username: this.username,\n          password: this.password\n        }).then(response => {\n          console.log(response);\n          this.registrationResult = response.data.msg;\n        }).catch(error => {\n          console.log(error);\n          this.registrationResult = error.response.data.detail;\n        });\n      } else if (formType === \"login\") {\n        axios.post(\"/api/login\", {\n          username: this.username,\n          password: this.password\n        }).then(response => {\n          console.log(response);\n          this.loginResult = response.data.msg;\n        }).catch(error => {\n          console.log(error);\n          this.loginResult = error.response.data.detail;\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","title","username","password","showForm","registrationResult","loginResult","methods","submitForm","formType","post","then","response","console","log","msg","catch","error","detail"],"sources":["/Users/zhaobin/Documents/WorkspaceP/fast_login_reg/login_reg_frontend/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1>{{ title }}</h1>\n    <hr>\n    <div v-if=\"showForm === 'register'\">\n      <h2>Register</h2>\n      <label>Username:</label>\n      <input type=\"text\" v-model=\"username\">\n      <label>Password:</label>\n      <input type=\"password\" v-model=\"password\">\n      <button @click=\"submitForm('register')\">Register</button>\n      <p v-if=\"registrationResult\">{{ registrationResult }}</p>\n    </div>\n   