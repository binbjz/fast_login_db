ult = true;\n            this.loginResult = response.data.msg;\n          })\n          .catch((error) => {\n            console.log(error);\n            this.loginResult = error.response.data.detail;\n          });\n    },\n  }\n};\n</script>\n\n<style scoped>\n.container {\n  max-width: 600px;\n  margin: 0 auto;\n}\n\nh1, h2 {\n  text-align: center;\n}\n\nhr {\n  margin: 2rem 0;\n  border-top: 1px solid #ccc;\n}\n\nlabel {\n  display: block;\n}\n\ninput[type=\"text\"], input[type=\"password\"] {\n  width: 100%;\n  padding: 0.5rem;\n  margin-bottom: 1rem;\n  font-size: 1.2rem;\n}\n\nbutton {\n  display: block;\n  margin: 2rem 0;\n  padding: 0.5rem 1rem;\n  background-color: #222;\n  color: #fff;\n  font-size: 1.2rem;\n  border: none;\n}\n\nbutton:hover {\n  background-color: #444;\n  cursor: pointer;\n}\n\np {\n  font-size: 1.2rem;\n  margin: 1rem 0;\n  padding: 0.5rem;\n  background-color: #eee;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n</style>\n"],"mappings":"AAiCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,uBAAuB;MAC9BC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,oBAAoB,EAAE,IAAI;MAC1BC,sBAAsB,EAAE,KAAK;MAC7BC,kBAAkB,EAAE,EAAE;MACtBC,aAAa,EAAE,IAAI;MACnBC,eAAe,EAAE,KAAK;MACtBC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQ,EAAE,SAAAA,CAAA,EAAY;MACpBb,KAAK,CAACc,IAAI,CAAC,eAAe,EAAE;QAC1BV,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC,EACIU,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QACrB,IAAI,CAACV,oBAAmB,GAAI,KAAK;QACjC,IAAI,CAACC,sBAAqB,GAAI,IAAI;QAClC,IAAI,CAACC,kBAAiB,GAAIQ,QAAQ,CAACd,IAAI,CAACiB,GAAG;MAC7C,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBJ,OAAO,CAACC,GAAG,CAACG,KAAK,CAAC;QAClB,IAAI,CAACb,kBAAiB,GAAIa,KAAK,CAACL,QAAQ,CAACd,IAAI,CAACoB,MAAM;MACtD,CAAC,CAAC;IACR,CAAC;IACDC,KAAK,EAAE,SAAAA,CAAA,EAAY;MACjBvB,KAAK,CAACc,IAAI,CAAC,YAAY,EAAE;QACvBV,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC,EACIU,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OA